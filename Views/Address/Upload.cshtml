@{
    ViewData["Title"] = "Upload";
}

<h1>Upload Kampagnemateriale</h1>

<hr />
<div class="text-center">
    <div class="row">
        <div class="col-md-5">
            <form method="post" enctype="multipart/form-data" asp-controller="Address" asp-action="Upload">
                <div class="form-group">
                    <div class="col-md-10">
                        <p>Upload fil</p>
                        <input class="form-control" name="files" type="file" />
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-10">
                        <input class="btn btn-success" type="submit" value="Upload" />
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>

<div>
    <a asp-action="Index">Tilbage til listen</a>
</div>